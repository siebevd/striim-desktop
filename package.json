{
	"name": "striim-desktop",
	"version": "1.0.0",
	"description": "striim desktop app ",
	"main": "main.js",
	"lint-staged": {
		"*.js": [
        "npm run pretty",
        "git add"
    ]
	},
	"scripts": {
		"dev": "webpack-dev-server --config config/webpack.config.dev.js",
		"build:prod": "NODE_ENV=production webpack --config config/webpack.config.prod.js",
		"build:dev": "webpack --config config/webpack.config.dev.js",
		"start": "electron main.js",
		"precommit": "lint-staged",
		"pretty": "prettier --write --use-tabs \"app/**/*.js\""
	},
	"author": "Siebe Van Dijck",
	"license": "ISC",
	"dependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-decorators-legacy": "^1.3.4",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"babel-preset-stage-0": "^6.24.1",
		"css-loader": "^0.28.7",
		"electron-devtools-installer": "^2.2.1",
		"electron-localshortcut": "^2.0.2",
		"fs": "0.0.1-security",
		"mobx": "^3.3.3",
		"mobx-react": "^4.3.5",
		"mobx-react-devtools": "^4.2.15",
		"postcss": "^6.0.14",
		"postcss-cssnext": "^3.0.2",
		"postcss-import": "^11.0.0",
		"postcss-loader": "^2.0.8",
		"postcss-modules-extract-imports": "^1.1.0",
		"postcss-modules-local-by-default": "^1.2.0",
		"postcss-modules-scope": "^1.1.0",
		"prop-types": "^15.6.0",
		"react": "^16.1.1",
		"react-dom": "^16.1.1",
		"react-hot-loader": "^3.1.3",
		"react-youtube": "^7.5.0",
		"style-loader": "^0.19.0",
		"webpack": "^3.8.1",
		"webpack-dev-server": "^2.9.4",
		"webpack-merge": "^4.1.1"
	},
	"devDependencies": {
		"husky": "^0.14.3",
		"lint-staged": "^6.0.0",
		"prettier": "1.9.2"
	}
}
